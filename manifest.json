{
    "manifest_version": 3,
    "name": "begining-tube",
    "description": "Make YT vids start at 0:00",
    "version": "1.0",
    "action": {
        "default_popup": "beg.html",
        "default_icon": "yt0-icon.png"
    },
    "content_scripts": [
        {
            "js": ["meat.js"],
            "matches": [
                "https://www.youtube.com/*",
                "https://youtu.be/*",
                "http://youtu.be/*"
            ]
        }
    ]
}